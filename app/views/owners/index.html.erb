<% provide(:title, 'Subscribers') %>
<h1>List of Owners</h1>


	
		
			<% @owners.each do |owner| %>
	
			<div class="container">
				<div class="row">
					<div class="col-sm-6">		
						<%= owner.name %>
					</div>
					<div class="col-sm-2">
						<%= link_to "Show", owner_path(owner), class:"btn btn-info"%> 
					</div>
				</div>	
			</div>			


			<% end %>

			<%= link_to "Add Subscriber", new_owner_path, class:"btn btn-primary"%>
		
	
	



