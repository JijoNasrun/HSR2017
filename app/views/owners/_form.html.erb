 <%= form_for(:owner) do |f| %>
      
      <%=f.label :Image %>
      <%= f.file_field :owner_image , required: "required"%>

      <%= f.label :Name %>
      <%= f.text_field :name %>

      <%= f.label :Ic_number %>
      <%= f.text_field :ic_number %>

      <%= f.label :email %>
      <%= f.email_field :email %>

      <%= f.label :Address %>
      <%= f.text_field :address %>  

      <%= f.submit yield(:button_text), class: "btn btn-primary" %>
    <% end %>